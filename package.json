{
  "name": "personal-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --ignore-path .gitignore --write **/*.{js,jsx,ts,tsx,json,css}",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "check-format": "prettier --ignore-path .gitignore --check **/*.{js,jsx,ts,tsx,json,css}",
    "check-lint": "eslint",
    "check-all": "yarn check-format && yarn check-types && yarn check-lint"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "next": "14.0.3",
    "next-intl": "^3.1.4",
    "prettier": "^3.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
